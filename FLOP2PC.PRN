 09EF C5        	PUSH	B
 09F0 47        	MOV	B,A		;STORE VALUE IN B
 09F1 E6F0      	ANI	0F0H		;HIGH NIBBLE
 09F3 0F        	RRC
 09F4 0F        	RRC
 09F5 0F        	RRC
 09F6 0F        	RRC
