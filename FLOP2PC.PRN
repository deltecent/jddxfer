 034E B7        	ora	a		;null terminates
 034F C8        	rz
                
 0350 47        	mov	b,a		;conOut wants character in b
 0351 CDB907    	call	conOut
 0354 23        	inx	h		;move to next byte
 0355 C34D03    	jmp	dispMsg
                
                ;-----------------------------------------------------------------------------
