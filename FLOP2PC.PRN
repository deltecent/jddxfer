 0857 78        	mov	a,DCM$LOG	;logon command seeks to track 0
 0858 CDD10A    	call	DSK$EX		;Z clear and error in A
 085B C8        	rz			;success
                
                ; restore failed
                
 085C 21D906    	lxi	h,mNoTrk0
                
 085F CD4D03    errMsg	call	dispMsg
 0862 315C09    	lxi	sp,ourStk	;initialize stack pointer
 0865 C31701    	jmp	getDrv		;start over asking for drive num
                
                ;--------------------------------------------------------------
                ; dSelDrv - Select and wait for drive specified in drvNum 
                ;    to be ready.
                ; On Entry:
                ;    drvNum = drive to be selected
                ; On Exit:
                ;    drive selected and ready, 1771 not busy
                ; Clobbers a
                ;--------------------------------------------------------------
