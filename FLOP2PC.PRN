 02F2 CD8F03    	call	rcvByte		;wait for 1 second of clear line
 02F5 C2F002    	jnz	clrRcv2
                
 02F8 E1        	pop	h		;restore pointer to the packet
 02F9 C37B02    	jmp	reSend		;re-send the packet
                
