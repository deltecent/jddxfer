 02E1 3D        	dcr	a
 02E2 C2F002    	jnz	clrRcv2		;no, go on
                
 02E5 3A1309    	lda	xmRetry		;failed three times in a row on block 1?
 02E8 D607      	sui	XMTRIES-3
