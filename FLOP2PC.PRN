 0AE9 1B        	DCX	D
 0AEA 7B        	MOV	A,E
 0AEB B2        	ORA	D
 0AEC C2E30A    	JNZ	WAIT
                
 0AEF 215C07    	LXI	H,mDDTO		;DISPLAY NO DD FOUND
 0AF2 CD4D03    	CALL	dispMsg
 0AF5 3E80      	MVI	A,ST$DNR	;SET DRIVE NOT READY
 0AF7 B7        	ORA	A		;CLEAR ZERO FLAG
 0AF8 C3020B    	JMP	EX$DONE
                
                ;*******( GET DOUBLE D STATUS )************************
                
 0AFB CD5E0A    HALTED	CALL	GET$CB
 0AFE 3A210B    	LDA	DCM$STS
 0B01 A7        	ANA	A		;TEST FOR ERRORS.
 0B02 E1        EX$DONE	POP	H		;RESTORE H
 0B03 C9        	RET			;RETURN TO CALLER.
                
                ;******************************************************
                ; BLOCK MOVE SUBROUTINE (Z80 BLOCK MOVE REGISTERS)    *
                ;******************************************************
                
 0B04 7E        BLK$MV:	MOV	A,M		;GET BYTE.
 0B05 23        	INX	H		;INC POINTER
 0B06 EB        	XCHG			;GET DESTINATION.
 0B07 77        	MOV	M,A		;PUT BYTE.
 0B08 23        	INX	H		;INC POINTER
 0B09 EB        	XCHG			;GET SOURCE.
 0B0A 0B        	DCX	B		;ONE LESS TO DO.
 0B0B 78        	MOV	A,B		;GET HI COUNT.
 0B0C B1        	ORA	C		;GET LO COUNT.
 0B0D C2040B    	JNZ	BLK$MV		;FINISH LOADING.
 0B10 C9        	RET
                
                ;******************************************************
                ; DISPLAY DCM ERROR IN A 			      *
                ;******************************************************
                
 0B11 218607    DCM$ER	LXI	H,mDNR		;DRIVE NOT READY
 0B14 E680      	ANI	ST$DNR
 0B16 C24D03    	JNZ	dispMsg
                
 0B19 C9        	RET
                
                ;******************************************************
                ; DOUBLE D - DCM COMMAND BLOCK BUFFER		      *
                ;******************************************************
                
 0B1A 00        DCM$CMD DB	0		;DCM COMMAND.
 0B1B 00        DCM$DRV DB	0		;DRIVE NUMBER.
 0B1C 00        DCM$TRK DB	0		;TRACK NUMBER.
 0B1D 00        DCM$SEC DB	0		;SECTOR NUMBER.
 0B1E 00        DCM$SP0 DB	0		;SPARE BYTE 0.
 0B1F 00        DCM$CHR DB	0		;LIST CHARACTER.
 0B20 00        DCM$MOD DB	00000000B	;MODE CONTROLS.
 0B21 00        DCM$STS DB	0		;COMMAND STATUS.
                
                ;******************************************************
                ; TRACK BUFFER RUNS FROM HERE TO THE END OF MEMORY    *
                ;******************************************************
                
 0B22 =         trkBuf	equ	$
                
 0B22           	end
