 086B CD8D0A    	call	SETDRV
 086E C9        	ret
                
                ;--------------------------------------------------------------
                ; dNxtSec - wait for next (any) sector. For the 1771, this
                ;    routine simply increments and wraps the sector number
                ;    passed in e, then falls into dWtSec to return bc with
                ;    the offset of the sector within the track buffer.
                ; On Entry:
                ;    Drive is selected, motor is running
                ;    e = most recent sector number
                ; On Exit:
                ;    e = next sector sector number
                ;    bc = byte offset of this sector in a track
                ;--------------------------------------------------------------
 086F 1C        dNxtSec	inr	e		;move to next staggered sector
 0870 1C        	inr	e		;skew of 2
                
 0871 3A8908    	lda	numSecs
 0874 47        	mov	b,a
 0875 04        	inr	b
 0876 7B        	mov	a,e		;see if past end
 0877 B8        	cmp	b		;verify between 1 and 26/50
 0878 DA7D08    	jc	dWtSec		;good sector, compute bc
                
 087B 1E01      dSec1	mvi	e,1		;restart at sector 1
                				;fall into dWtSec
                
                ;--------------------------------------------------------------
                ; dWtSec - wait for sector specified in e. For the DD
                ;    controller, this routine simply returns the offset
                ;    of the sector within this track in bc.
                ; On Entry:
                ;    Drive is selected, motor is running
                ;    e = desired sector number (must be 1-50 for DD)
                ; On Exit:
                ;    e = sector number
                ;    bc = byte offset of this sector in a track
                ;--------------------------------------------------------------
 087D 7B        dWtSec	mov	a,e		;save the computed sector
                
                ; compute bc=128*(sector-1)
                
 087E D601      	sui	1		;zero index and clear carry
 0880 1F        	rar
 0881 47        	mov	b,a		;b=sector >> 1
                
 0882 1F        	rar			;put lsbit into msbit	
 0883 E680      	ani	80h
 0885 4F        	mov	c,a		;bc=128*e
 0886 C9        	ret
                
                
